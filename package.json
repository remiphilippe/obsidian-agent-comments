{
	"name": "obsidian-agent-comments",
	"version": "0.1.0",
	"description": "Bidirectional inline comment threads between humans and AI agents on markdown documents",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "vitest run",
		"lint": "eslint .",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"comments",
		"ai",
		"agent"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/remiphilippe/obsidian-agent-comments.git"
	},
	"author": "Remi Philippe",
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@types/diff-match-patch": "^1.0.36",
		"@types/node": "^22.0.0",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"jiti": "2.6.1",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"vitest": "^4.0.0"
	},
	"dependencies": {
		"@flatten-js/interval-tree": "^1.1.0",
		"diff-match-patch": "^1.0.5",
		"obsidian": "latest"
	}
}
